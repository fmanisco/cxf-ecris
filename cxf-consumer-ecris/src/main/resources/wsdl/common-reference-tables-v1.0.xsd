<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:commons="http://ec.europa.eu/ECRIS/commons-v1.0" xmlns:crt="http://ec.europa.eu/ECRIS/common-reference-tables-v1.0" targetNamespace="http://ec.europa.eu/ECRIS/common-reference-tables-v1.0" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xsd:import namespace="http://ec.europa.eu/ECRIS/commons-v1.0" schemaLocation="commons-v1.0.xsd"/>
	<!-- Simple type definitions START-->
	<xsd:simpleType name="NonBindingExternalReferenceType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">A simple marker type, used to indicate that an extending type can be used as a non-binding reference in external XML documents.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="commons:StringType"/>
	</xsd:simpleType>
	<xsd:simpleType name="MemberStateCodeType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">A type for providing the 2-letter ISO code a a Member State. The following values are expected:
			AT, BE, BG, CY, CZ, DE, DK, EE, ES, FI, FR, GB, GR, HR, HU, IE, LT, LU, LV, MT, NL, PL, PT, RO, SE, SI, SK
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="commons:StringType">
			<xsd:length value ="2"/>
			<xsd:pattern value="[A-Z]{2}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Central Authority related simple types START-->
	<xsd:simpleType name="CentralAuthorityTechnicalIdentifierType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">The technical identifier for the CentralAuthorityType</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="commons:IDType">
			<xsd:length value="8"/>
			<xsd:pattern value="CA-[0-9]{2}-[A-Z]{2}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Central Authority related simple types END-->
	<!-- Country  related simple types START-->
	<xsd:simpleType name="CountryTechnicalIdentifierType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">The technical identifier for the CountryType</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="commons:IDType">
			<xsd:length value="13"/>
			<xsd:pattern value="CO-[0-9]{2}-[0-9]{3}-[A-Z]{3}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CountryReferenceType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">The type used to refer internally an instance of a CountryType</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="commons:IDRefType">
			<xsd:length value="13"/>
			<xsd:pattern value="CO-[0-9]{2}-[0-9]{3}-[A-Z]{3}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CountryExternalReferenceType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">The type used to refer in external documents an instance of a CountryType</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="crt:NonBindingExternalReferenceType">
			<xsd:length value="13"/>
			<xsd:pattern value="CO-[0-9]{2}-[0-9]{3}-[A-Z]{3}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CountryISO3166-2NumberType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">The ISO-3166-2 country number (3-digit number)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="commons:StringType">
			<xsd:length value="3"/>
			<xsd:pattern value="[0-9]{3}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CountryISO3166-2CodeType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">The ISO-3166-2 country code (3-letter code)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="commons:StringType">
			<xsd:length value="3"/>
			<xsd:pattern value="[A-Z]{3}"/>
