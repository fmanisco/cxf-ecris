            </xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:restriction base="msg:AbstractMessageType">
				<xsd:sequence>
					<xsd:element name="MessageMetaData" type="msg:CorrelatingMessageMetaDataType"/>
					<xsd:element name="MessageData" type="msg:FunctionalErrorDataType"/>
				</xsd:sequence>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="CancellationMessageDataType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">This type represents the data content of the "cancellation" message.
            </xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:restriction base="msg:AbstractBusinessMessageDataType">
				<xsd:sequence>
					<xsd:element name="MessageDataContactPerson" type="msg:ContactPersonType" minOccurs="0"/>
					<!-- START: using a string type not collapsing white-space characters (v0.22 of this file) -->
					<xsd:element name="MessageDataRemarks" type="commons:UncollapsedMultilingualTextType" minOccurs="0"/>
					<!-- END -->
				</xsd:sequence>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="CancellationMessageType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">The "Cancellation" message is used to cancel a previously transmitted ECRIS
                message. Message types that can be cancelled are Requests, Request Responses, Notifications and
                Notification Responses
            </xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:restriction base="msg:AbstractMessageType">
				<xsd:sequence>
					<xsd:element name="MessageMetaData" type="msg:CancellationMessageMetaDataType"/>
					<xsd:element name="MessageData" type="msg:CancellationMessageDataType"/>
				</xsd:sequence>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- Message type definitions for functional errors and cancellation END -->
	<!-- Message type definitions required for Request kinematics START -->
