	<xsd:complexType name="RequestResponseMessageDataType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">This type represents the data content of the "Request Response" message. It
                combines in a single type of message the following business responses: request denial, request problem
                and request response. The response type indicates the resulting response.
            </xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:restriction base="msg:AbstractBusinessMessageDataType">
				<xsd:sequence>
					<xsd:element name="MessageDataContactPerson" type="msg:ContactPersonType" minOccurs="0"/>
					<xsd:element name="MessageDataPerson" type="dm:PersonType"/>
					<!-- START: using a string type not collapsing white-space characters (v0.22 of this file) -->
					<xsd:element name="MessageDataRemarks" type="commons:UncollapsedMultilingualTextType" minOccurs="0"/>
					<!-- END -->
					<xsd:element name="MessageDataConviction" type="dm:ConvictionType" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element name="MessageDataOtherMemberState" type="crt:MemberStateCodeType" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element name="MessageDataRequestResponseTypeReference" type="crt:RequestResponseTypeExternalReferenceType"/>
				</xsd:sequence>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="RequestResponseMessageType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">This type of message allows a Member State to transmit the response to the
                request. It can contain: (1) information on convictions extracted from the national criminal records
