					<!-- START: limiting max length to 400 chars (v0.22 of this file) -->
					<xsd:element name="MessageDataAccusation" type="commons:MultilingualTextType400Chars" minOccurs="0" />
					<!-- END -->
					<xsd:element name="MessageDataRequestingAuthority" type="msg:RequestingAuthorityType"/>
					<!-- START: limiting max length to 50 chars (v0.22 of this file) -->
					<xsd:element name="MessageDataCaseReferenceNumber" type="commons:RestrictedStringType50Chars" minOccurs="0" />
					<!-- END -->
					<xsd:element name="MessageDataConcernedPersonConsent" type="commons:YesNoUnknownStringEnumerationType"/>
					<xsd:element name="MessageDataUrgency" type="commons:YesNoUnknownStringEnumerationType"/>
