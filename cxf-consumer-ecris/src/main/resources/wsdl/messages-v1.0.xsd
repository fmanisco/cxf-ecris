	<xsd:complexType name="RequestDeadlineMessageType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">This type of message allows a Member State to transmit the legal deadline
                for the response to the requesting Member State.
            </xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:restriction base="msg:AbstractMessageType">
				<xsd:sequence>
					<xsd:element name="MessageMetaData" type="msg:CorrelatingMessageMetaDataType"/>
