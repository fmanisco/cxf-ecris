	<xsd:complexType name="RequestDeadlineMessageDataType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">This type represents the data content of the "Request Deadline" message.
            </xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:restriction base="msg:AbstractBusinessMessageDataType">
				<xsd:sequence>
					<xsd:element name="MessageDataContactPerson" type="msg:ContactPersonType" minOccurs="0"/>
					<!-- START: using a string type not collapsing white-space characters (v0.22 of this file) -->
